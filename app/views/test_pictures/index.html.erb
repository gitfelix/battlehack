<h1>Listing test_pictures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_pictures.each do |test_picture| %>
      <tr>
        <td><%= link_to 'Show', test_picture %></td>
        <td><%= link_to 'Edit', edit_test_picture_path(test_picture) %></td>
        <td><%= link_to 'Destroy', test_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test picture', new_test_picture_path %>
